{
    "version": 2,
    "builds": [
        {
            "src": "./index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/"
        }
    ],
    "headers": [
        {
            "key": "Content-Security-Policy",
            "value": "default-src 'self'; style-src-elem 'self' https://cdn.jsdelivr.net"
        }
    ]
}